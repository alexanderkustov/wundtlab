function ApplicationWindow(){function e(){t.loadRssFeed({success:function(e){a.refreshRssTable(e)}})}var t=require("services/rss"),i=require("ui/common/MasterView"),n=require("ui/common/DetailView"),r=Ti.UI.createWindow({backgroundColor:"#fff"}),a=new i,o=new n,s=Ti.UI.createWindow({title:"RSS Reader",layout:"vertical"}),d=Ti.UI.createButton({backgroundImage:"images/refresh_icon.png",width:40,height:40,top:0,right:10});d.addEventListener("click",function(){e()}),s.add(d),s.add(a);var c=Ti.UI.createWindow();c.add(o);var l=Ti.UI.MobileWeb.createNavigationGroup({window:s});return r.add(l),a.addEventListener("itemSelected",function(e){o.showArticle(e.link),l.open(c)}),e(),r}module.exports=ApplicationWindow;